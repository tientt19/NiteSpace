# platform :ios, '9.0'

target 'niteSpace' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for niteSpace
  # Networking
  pod 'Alamofire'
  pod 'Kingfisher'
  pod 'ReachabilitySwift'

  # Firebase SDK
  pod 'Firebase/Analytics'
  pod 'Firebase/Crashlytics'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Core'

  # Google SDK
  pod 'GoogleSignIn'

  # Support
  pod 'KeychainAccess'
  pod 'SnapKit'
end
